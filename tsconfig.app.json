{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./wwwroot/clientapp/out-tsc/app",
    "types": [],
    "skipLibCheck": true,
    "experimentalDecorators": true
  },
  "files": [
    "ClientApp/main.ts",
    "ClientApp/polyfills.ts"
