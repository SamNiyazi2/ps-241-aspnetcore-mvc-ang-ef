
<!doctype html>
<html>
<head>
    <title>p3132 - Dutch Treat - @ViewBag.title</title>
    <link href="~/css/site.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <header><h1>Welcome to Dutch Treat</h1></header>
    <menu>
        <ul>
            <li><a asp-controller="App" asp-action="Index">Home</a></li>
            <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
            <li><a asp-controller="App" asp-action="About">About</a></li>
            <li><a asp-controller="App" asp-action="ThrowError">Error</a></li>
        </ul>
    </menu>
    <section>
        <h2>@ViewBag.title</h2>

        @RenderBody()

    </section>

    <footer>@@ @DateTime.Now.Year Dutch Treat LLC</footer>

    <script src="~/node_modules/jquery/dist/jqeury.min.js"></script>
    <script src="~/js/index.js"></script>

</body>
</html>